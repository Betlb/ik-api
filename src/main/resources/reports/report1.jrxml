	<subDataset name="Table Dataset 1" uuid="4f85650c-dba7-4ce1-a4f8-1801a57a45a6">
		<parameter name="id" class="java.lang.Long">
			<defaultValueExpression><![CDATA[187]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT to_char(d.yil::date,'YYYY')::int as yil,
       d.kullanilan_izin_hakki,
       d.kalan_izin_hakki
FROM yillik_izin_hakki_detay_snapshot as d
WHERE exists(select 0
             from izin as i
             where d.personel_id = i.personel_id
               and i.id = d.izin_id )
  AND d.izin_id =  $P{id}]]>
		</queryString>
		<field name="yil" class="java.lang.Long"/>
		<field name="kullanilan_izin_hakki" class="java.lang.Long"/>
		<field name="kalan_izin_hakki" class="java.lang.Long"/>
	</subDataset>
	<parameter name="id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
    p.tc,
    p.adres,
    p.tel,
    p.mail,
    p.mazeret_izin,
    p.adi,
    p.soyadi,
    to_char(p.is_giris_tarihi, 'DD/MM/YYYY') as is_giris_tarihi,
    p.gorev,
    p.birim_ad,
    i.toplam_yillik_izin_hakki,
    i.izin_turu,
    i.izin_gun,
    i.sebep,
    i.izin_turu,
    i.izni_veren,
    i.personel_id,
    to_char(i.izin_baslangic_tarihi, 'DD/MM/YYYY') as izin_baslangic_tarihi,
    to_char(i.izin_bitis_tarihi, 'DD/MM/YYYY') as izin_bitis_tarihi,
    i.id,
    to_char(i.izin_olusturulma_tarihi, 'DD/MM/YYYY') as izin_olusturulma_tarihi,
    i.hafta_tatili,
    i.genel_tatil,
    i.kurum_sicil_no,
    i.hafta_tatili,
    i.genel_tatil
FROM
    public."personel" p
JOIN
    izin i ON p.id = i.personel_id
WHERE
    i.id = $P{id}]]>
	</queryString>
